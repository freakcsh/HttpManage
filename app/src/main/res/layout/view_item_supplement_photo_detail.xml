<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <ImageView
        android:id="@+id/imageViewSupplementPhotoDetailItemBg"
        android:layout_width="89pt"
        android:layout_height="89pt"
        android:layout_marginTop="13pt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailItemContext"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10pt"
        android:layout_marginTop="7pt"
        android:layout_marginRight="9pt"
        android:gravity="center|left"
        android:includeFontPadding="false"
        android:text="南非红西柚南非红西柚葡萄 萄红西柚葡萄"
        android:textColor="@color/color_353535"
        android:textSize="15pt"
        app:layout_constraintLeft_toRightOf="@+id/imageViewSupplementPhotoDetailItemBg"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewSupplementPhotoDetailItemBg" />

    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailItemContext2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7pt"
        android:layout_marginRight="9pt"
        android:gravity="center|left"
        android:includeFontPadding="false"
        android:text="冷藏后的树莓简直是宝藏冷藏后 的树莓简直是宝藏"
        android:textColor="@color/color_999999"
        android:textSize="12pt"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewSupplementPhotoDetailItemBg"
        app:layout_constraintLeft_toLeftOf="@+id/textViewViewSupplementPhotoDetailItemContext"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailItemSkuTip"
        android:layout_width="38pt"
        android:layout_height="24pt"
        android:layout_marginTop="8pt"
        android:background="@drawable/bg_shape_e8fff7_5"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="规格"
        android:textColor="@color/color_24BA85"
        android:textSize="13pt"
        app:layout_constraintLeft_toLeftOf="@+id/imageViewSupplementPhotoDetailItemBg"
        app:layout_constraintTop_toBottomOf="@+id/imageViewSupplementPhotoDetailItemBg" />

    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailItemSku"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8pt"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="罐装250g"
        android:textColor="@color/color_353535"
        android:textSize="13pt"
        app:layout_constraintLeft_toLeftOf="@+id/textViewViewSupplementPhotoDetailItemSkuTip"
        app:layout_constraintTop_toBottomOf="@+id/textViewViewSupplementPhotoDetailItemSkuTip" />

    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailItemBarCodeTip"
        android:layout_width="38pt"
        android:layout_height="24pt"
        android:layout_marginLeft="60pt"
        android:layout_marginTop="8pt"
        android:background="@drawable/bg_shape_e8fff7_5"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="条码"
        android:textColor="@color/color_24BA85"
        android:textSize="13pt"
        app:layout_constraintLeft_toRightOf="@+id/textViewViewSupplementPhotoDetailItemSkuTip"
        app:layout_constraintTop_toBottomOf="@+id/imageViewSupplementPhotoDetailItemBg" />

    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailItemBarCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8pt"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="1234561234560"
        android:textColor="@color/color_353535"
        android:textSize="14pt"
        app:layout_constraintLeft_toLeftOf="@+id/textViewViewSupplementPhotoDetailItemBarCodeTip"
        app:layout_constraintTop_toBottomOf="@+id/textViewViewSupplementPhotoDetailItemBarCodeTip" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayoutDownOrUp"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:paddingLeft="15pt"
        android:paddingRight="12pt"
        app:layout_constraintBottom_toBottomOf="@+id/textViewViewSupplementPhotoDetailItemBarCode"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textViewViewSupplementPhotoDetailItemBarCodeTip">

        <ImageView
            android:id="@+id/imageViewDownOrUp"
            android:layout_width="12pt"
            android:layout_height="7pt"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1pt"
        app:layout_goneMarginTop="8pt"
        android:layout_marginTop="8pt"
        app:layout_constraintTop_toBottomOf="@+id/textViewViewSupplementPhotoDetailItemBarCode"
        android:background="@color/color_e6e6e6"/>

</androidx.constraintlayout.widget.ConstraintLayout>