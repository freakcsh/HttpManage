<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailSkuItemSku"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8pt"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="罐装250g"
        android:layout_marginTop="8pt"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/color_353535"
        android:textSize="13pt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/textViewViewSupplementPhotoDetailSkuItemBarCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="1234561234560"
        android:layout_marginLeft="60pt"
        android:textColor="@color/color_353535"
        android:textSize="14pt"
        app:layout_constraintLeft_toRightOf="@+id/textViewViewSupplementPhotoDetailSkuItemSku"
        app:layout_constraintTop_toTopOf="@+id/textViewViewSupplementPhotoDetailSkuItemSku" />

</androidx.constraintlayout.widget.ConstraintLayout>